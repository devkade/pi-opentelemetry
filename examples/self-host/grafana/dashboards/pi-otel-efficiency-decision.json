{
  "id": null,
  "uid": "pi-otel-efficiency-decision",
  "title": "Pi OTel Efficiency & Decision",
  "tags": ["pi", "otel", "efficiency"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "1m",
  "time": {
    "from": "now-30d",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Cost / Turn (1h)",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_cost_usage_USD_total[1h])) / clamp_min(sum(increase(pi_turn_count_total[1h])), 1)",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" }, "overrides": [] },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Cost / Successful Tool Result (1h)",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_cost_usage_USD_total[1h])) / clamp_min(sum(increase(pi_tool_result_count_total{success=\"true\"}[1h])), 1)",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" }, "overrides": [] },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Turns / Session (1h)",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_turn_count_total[1h])) / clamp_min(sum(increase(pi_session_count_total[1h])), 1)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Tool Calls / Turn (1h)",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_tool_call_count_total[1h])) / clamp_min(sum(increase(pi_turn_count_total[1h])), 1)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Output / Input Token Ratio (1h)",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_token_usage_tokens_total{type=\"output\"}[1h])) / clamp_min(sum(increase(pi_token_usage_tokens_total{type=\"input\"}[1h])), 1)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Turn p95 (5m)",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(pi_turn_duration_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Cost / Turn Trend (1h)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_cost_usage_USD_total[1h])) / clamp_min(sum(increase(pi_turn_count_total[1h])), 1)",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Turns / Session Trend (1h)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_turn_count_total[1h])) / clamp_min(sum(increase(pi_session_count_total[1h])), 1)",
          "refId": "A"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Cost by Provider / Model (24h)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_cost_usage_USD_total[24h])) by (provider, model)",
          "legendFormat": "{{provider}}/{{model}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Cost / Turn by Provider / Model (6h)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(increase(pi_cost_usage_USD_total[6h])) by (provider, model) / clamp_min(sum(increase(pi_turn_count_total[6h])) by (provider, model), 1)",
          "legendFormat": "{{provider}}/{{model}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Tool Success Rate by Tool (6h)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "100 * sum(increase(pi_tool_result_count_total{success=\"true\"}[6h])) by (tool_name) / clamp_min(sum(increase(pi_tool_result_count_total[6h])) by (tool_name), 1)",
          "legendFormat": "{{tool_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Turn Tail Risk (p95 / p50)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(pi_turn_duration_seconds_bucket[5m])) by (le)) / clamp_min(histogram_quantile(0.50, sum(rate(pi_turn_duration_seconds_bucket[5m])) by (le)), 0.001)",
          "legendFormat": "p95/p50",
          "refId": "A"
        },
        {
          "expr": "vector(2)",
          "legendFormat": "risk threshold",
          "refId": "B"
        }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    }
  ]
}
