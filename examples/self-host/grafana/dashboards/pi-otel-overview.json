{
  "id": null,
  "uid": "pi-otel-overview",
  "title": "Pi OTel Overview",
  "tags": ["pi", "otel"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Sessions (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "sum(increase(pi_session_count_total[1h]))", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "textMode": "auto" }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Turns (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "sum(increase(pi_turn_count_total[1h]))", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "textMode": "auto" }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Tool Calls (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "sum(increase(pi_tool_call_count_total[1h]))", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "textMode": "auto" }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Cost USD (1h)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "sum(increase(pi_cost_usage_USD_total[1h]))", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD"
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "textMode": "auto" }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Token Usage by Type (5m rate)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "sum(rate(pi_token_usage_tokens_total[5m])) by (type)", "legendFormat": "{{type}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops" },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Tool Calls by Name (5m increase)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "sum(increase(pi_tool_call_count_total[5m])) by (tool_name)", "legendFormat": "{{tool_name}}", "refId": "A" }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Turn Duration p95 (5m)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(pi_turn_duration_seconds_bucket[5m])) by (le))", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Tool Duration p95 by Tool (5m)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(pi_tool_duration_seconds_bucket[5m])) by (le, tool_name))", "legendFormat": "{{tool_name}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    }
  ]
}
